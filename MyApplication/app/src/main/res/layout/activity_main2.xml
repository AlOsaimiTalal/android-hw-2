<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:backgroundTint="#FBFBFB"
    tools:context=".MainActivity2">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="175dp"
        android:layout_toEndOf="@+id/textew2"
        android:backgroundTintMode="screen"
        android:fontFamily="casual"
        android:text="name"
        android:textColor="#000000"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/MyName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textew2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="45dp"
        android:fontFamily="@font/amethysta"
        android:text="Age :"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/MyAge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/MyName"
        android:layout_alignParentStart="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="45dp"
        android:fontFamily="@font/amethysta"
        android:text="Your Job :"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/jdvn"
        android:layout_width="55dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textew2"
        android:layout_marginStart="10dp"
        android:layout_marginTop="45dp"
        android:layout_toEndOf="@+id/MyName"
        android:fontFamily="casual"
        android:text="age"
        android:textColor="#000000"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/textew2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="175dp"
        android:fontFamily="@font/amethysta"
        android:text="Name : "
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="20dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="13dp"
        app:srcCompat="@drawable/paci" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="131dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/jdvn"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="39dp"
        android:layout_toEndOf="@+id/MyAge"
        android:fontFamily="casual"
        android:text="job"
        android:textColor="#000000"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/MyAge"
        android:layout_alignParentStart="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="45dp"
        android:fontFamily="@font/amethysta"
        android:text="Phone NO :"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="119dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_marginStart="10dp"
        android:layout_marginTop="40dp"
        android:layout_toEndOf="@+id/textView2"
        android:fontFamily="casual"
        android:text=" Number"
        android:textColor="#000000"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="45dp"
        android:fontFamily="@font/amethysta"
        android:text="Email :"
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_marginStart="10dp"
        android:layout_marginTop="45dp"
        android:layout_toEndOf="@+id/textView5"
        android:fontFamily="casual"
        android:text="email"
        android:textColor="#000000"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="217dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="71dp"
        android:fontFamily="@font/arizonia"
        android:text="Thank You"
        android:textColor="#000000"
        android:textSize="40dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="90dp"
        android:layout_height="69dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="146dp"
        android:layout_marginBottom="0dp"
        app:srcCompat="@drawable/happyface" />

</RelativeLayout>